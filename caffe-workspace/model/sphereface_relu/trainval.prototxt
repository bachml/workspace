layer {
  name: "fc6_xxx"
  type: "InnerProduct"
  bottom: "fc5"
  top: "fc6_xxx"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  inner_product_param {
     num_output: 10572
     weight_filler { type: "xavier" std: 0.010 }
     bias_filler { type: "constant" value: 0 }
  }
}

layer {
  name: "softmax_loss"
  type: "SoftmaxWithLoss"
  bottom: "fc6_xxx"
  bottom: "label"
  top: "softmax_loss"
}

layer {
  name: "center_loss_xxx"
  type: "CenterLoss"
  bottom: "fc5"
  bottom: "label"
  top: "center_loss_xxx"
  param {
    lr_mult: 1
    decay_mult: 2
  }
  center_loss_param {
    num_output: 10572
    center_filler {
      type: "xavier"
    }
  }
  loss_weight: 0.008
}
